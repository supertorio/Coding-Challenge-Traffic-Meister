{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "semi": "warn",
    "no-console": "error",
    "no-unused-expressions": "off",
    "eqeqeq": ["error", "always"],
    "prefer-const": "error"
  }
}
